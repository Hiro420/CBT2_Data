      },
      "triggerActions": [
        {
          "$type": "ConfigGadgetDoAttackEvent",
          "attackEvent": "ATK01",
        },
        {
          "$type": "ConfigGadgetTriggerAbility",
          "abilityName": "Effect_Fischl_ShootArrow_Hit_Fx",
        },
      ],
      "fireAISoundEvent": true,
    },
    "abilities": [
      {
        "abilityID": "Trail_Fx",

        "abilityName": "Effect_Fischl_ShootArrow_Trail_Light_Fx"
      },
      {
        "abilityID": "Hit_Fx",

