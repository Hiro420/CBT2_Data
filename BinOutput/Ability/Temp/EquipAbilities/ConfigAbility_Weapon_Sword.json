                    "overrideWeaponElement": true,
                },
                "Fire":
                {
                    "elementType": "Fire",
                    "elementDurability": 20,
                    "overrideWeaponElement": true,
                },
                "Ice":
                {
                    "elementType": "Ice",
                    "elementDurability": 20,
                    "overrideWeaponElement": true,
                },
                "Wind":
                {
                    "elementType": "Wind",
                    "elementDurability": 20,
                    "overrideWeaponElement": true,
                },
                "Electric":
                {
                    "elementType": "Electric",
                    "elementDurability": 20,
                    "overrideWeaponElement": true,
                },
                "Rock":
                {
                    "elementType": "Rock",
                    "elementDurability": 20,
                    "overrideWeaponElement": true,
                },
                "Reset":
                {
                    "onAdded":
                    [
                        {
                            "$type": "AddGlobalValue",
                            "key": "_ABILITY_WEAPONSKILL_ChangeColor_COUNT",
                            "value": -10,
                            "useLimitRange": true,
                            // "maxValue":7,
                            "minValue": 0,
                        },
                    ],
                },
            },
