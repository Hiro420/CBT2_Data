    {
        "Default":
        {
            "$type": "ConfigAbility",
            "abilityName": "Weapon_Sword_ChangeColor",
            "onAbilityStart":
            [
                {
                    "$type": "AddGlobalValue",
                    "key": "_ABILITY_WEAPONSKILL_ChangeColor_COUNT",
                    "value": 1,
                    // "useLimitRange":true,
                    // "maxValue":7,
                    // "minValue":0,
                },
            ],
            "abilityMixins":
            [

            ],
            "onAdded":
            [
                {
                    "$type": "AttachModifier",
                    "modifierName": "handler",
                },
            ],

            "modifiers":
            {
                "handler":
                {
                    "modifierMixins": //用addability时这里用abilitymixin有点问题。。只能改用modifierability
                    [
                        {
                            "$type": "AttachModifierToSelfGlobalValueMixin",
                            "globalValueKey": "_ABILITY_WEAPONSKILL_ChangeColor_COUNT",
                            "defaultGlobalValueOnCreate": 0,
                            "valueSteps":
                            [
                                -0.5,
                                0.5,
                                1.5,
                                2.5,
                                3.5,
                                4.5,
                                5.5,
                                6.5,
                                100
                            ],
                            "removeAppliedModifier": true,
                            "modifierNameSteps":
                            [
                                "No_Color",
                                "Water",
                                "Fire",
                                "Ice",
                                "Wind",
                                "Electric",
                                "Rock",
                                "Reset"
                            ],
                        },
                    ],
                },
